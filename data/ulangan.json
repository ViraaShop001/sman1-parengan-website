      "status": "selesai",
      "soal": [
        {
          "id": "soal_001",
          "nomor": 1,
          "pertanyaan": "Nilai sin 30° adalah...",
          "tipe": "pilihan_ganda",
          "pilihan": [
            {"id": "A", "teks": "1/2"},
            {"id": "B", "teks": "√3/2"},
            {"id": "C", "teks": "√2/2"},
            {"id": "D", "teks": "1"}
          ],
          "jawaban_benar": "A",
          "nilai": 10,
          "pembahasan": "Sinus 30° memiliki nilai 1/2 berdasarkan tabel trigonometri standar."
        },
        {
          "id": "soal_002",
          "nomor": 2,
          "pertanyaan": "Jika cos x = 1/2, maka nilai x adalah...",
          "tipe": "pilihan_ganda",
          "pilihan": [
            {"id": "A", "teks": "30°"},
            {"id": "B", "teks": "45°"},
            {"id": "C", "teks": "60°"},
            {"id": "D", "teks": "90°"}
          ],
          "jawaban_benar": "C",
          "nilai": 10,
          "pembahasan": "Cosinus 60° = 1/2. Nilai x yang memenuhi adalah 60°."
        },
        {
          "id": "soal_003",
          "nomor": 3,
          "pertanyaan": "Nilai dari tan 45° adalah...",
          "tipe": "pilihan_ganda",
          "pilihan": [
            {"id": "A", "teks": "0"},
            {"id": "B", "teks": "1"},
            {"id": "C", "teks": "√3"},
            {"id": "D", "teks": "Tidak terdefinisi"}
          ],
          "jawaban_benar": "B",
          "nilai": 10,
          "pembahasan": "Tangen 45° = 1, karena sin 45°/cos 45° = (√2/2)/(√2/2) = 1."
        },
        {
          "id": "soal_004",
          "nomor": 4,
          "pertanyaan": "Sin²x + cos²x = ...",
          "tipe": "pilihan_ganda",
          "pilihan": [
            {"id": "A", "teks": "0"},
            {"id": "B", "teks": "1"},
            {"id": "C", "teks": "2"},
            {"id": "D", "teks": "sin 2x"}
          ],
          "jawaban_benar": "B",
          "nilai": 10,
          "pembahasan": "Ini adalah identitas trigonometri dasar: sin²x + cos²x = 1."
        },
        {
          "id": "soal_005",
          "nomor": 5,
          "pertanyaan": "Nilai dari sin 90° adalah...",
          "tipe": "pilihan_ganda",
          "pilihan": [
            {"id": "A", "teks": "0"},
            {"id": "B", "teks": "1/2"},
            {"id": "C", "teks": "√2/2"},
            {"id": "D", "teks": "1"}
          ],
          "jawaban_benar": "D",
          "nilai": 10,
          "pembahasan": "Sinus 90° = 1. Ini adalah nilai dasar trigonometri yang perlu dihafal."
        }
      ],
